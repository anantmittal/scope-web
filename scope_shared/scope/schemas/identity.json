{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"$id": "https://uwscope.org/schemas/identity",
	"title": "IIdentity",
	"description": "Identity of the user",
	"type": "object",
	"properties": {
		"_id": {
			"type": "string"
		},
		"_type": {
			"$ref": "/schemas/enums#/properties/identityType"
		},
		"_set_id": {
			"type": "string"
		},
		"_rev": {
			"type": "number"
		},
		"identityId": {
			"type": "string"
		},
		"collection": {
			"type": "string"
		},
		"name": {
			"type": "string"
		}
	},
	"additionalProperties": false,
	"required": ["_type", "identityId", "name"]
}
