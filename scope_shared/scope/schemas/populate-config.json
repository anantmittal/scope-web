{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"$id": "https://uwscope.org/schemas/populate-config",
	"title": "PopulateConfig",
	"type": "object",
	"properties": {
		"patients": {
			"type": "object",
			"properties": {
				"create": {
					"type": "array",
					"items": {
						"type": "object",
						"properties": {
							"name": {
								"type": "string"
							},
							"MRN": {
								"type": "string"
							}
						},
						"additionalProperties": false,
						"required": ["name", "MRN"]
					},
					"minItems": 0
				},
				"existing": {
					"type": "array",
					"items": {
						"type": "object",
						"properties": {
							"name": {
								"type": "string"
							},
							"MRN": {
								"type": "string"
							},
							"patientId": { "type": "string" }
						},
						"additionalProperties": false,
						"required": ["name", "MRN", "patientId"]
					},
					"minItems": 0
				}
			},
			"required": ["create", "existing"]
		},
		"providers": {
			"type": "object",
			"properties": {
				"create": {
					"type": "array",
					"items": {
						"type": "object",
						"properties": {
							"name": {
								"type": "string"
							},
							"role": {
								"$ref": "/schemas/enums#/properties/providerRole"
							}
						},
						"additionalProperties": false,
						"required": ["name", "role"]
					},
					"minItems": 0
				},
				"existing": {
					"type": "array",
					"items": {
						"type": "object",
						"properties": {
							"name": {
								"type": "string"
							},
							"providerId": {
								"type": "string"
							},
							"role": {
								"$ref": "/schemas/enums#/properties/providerRole"
							}
						},
						"required": ["name", "providerId", "role"]
					},
					"minItems": 0
				}
			},
			"required": ["create", "existing"]
		}
	},
	"additionalProperties": false,
	"required": ["patients", "providers"]
}
