{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"$id": "https://uwscope.org/schemas/scheduled-assessment",
	"title": "IScheduledAssessment",
	"description": "IScheduledAssessment Type",
	"type": "object",
	"properties": {
		"_id": {
			"type": "string"
		},
		"_type": {
			"const": "scheduledAssessment"
		},
		"_set_id": {
			"type": "string"
		},
		"_rev": {
			"type": "number"
		},
		"scheduleAssessmentId": {
			"$ref": "/schemas/scheduled-item#/properties/scheduleId"
		},
		"dueDate": {
			"$ref": "/schemas/scheduled-item#/properties/dueDate"
		},
		"dueType": {
			"$ref": "/schemas/scheduled-item#/properties/dueType"
		},
		"assessmentId": {
			"type": "string"
		},
		"completed": {
			"type": "boolean"
		}
	},
	"additionalProperties": false,
	"required": ["_type", "dueDate", "dueType", "assessmentId", "completed"]
}
