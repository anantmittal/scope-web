{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"$id": "https://uwscope.org/schemas/assessment-log",
	"title": "IAssessmentLog",
	"description": "IAssessmentLog Type",
	"type": "object",
	"$comment": "property pointValues not clear : { \"$ref\": \"\"}",
	"properties": {
		"_id": {
			"type": "string"
		},
		"_type": {
			"const": "assessmentLog"
		},
		"_set_id": {
			"type": "string"
		},
		"_rev": {
			"type": "number"
		},
		"assessmentLogId": {
			"$ref": "/schemas/log#/properties/logId"
		},
		"recordedDateTime": {
			"$ref": "/schemas/log#/properties/recordedDateTime"
		},
		"comment": {
			"$ref": "/schemas/log#/properties/comment"
		},
		"scheduledAssessmentId": {
			"type": "string"
		},
		"assessmentId": {
			"type": "string"
		},
		"completed": {
			"type": "boolean"
		},
		"patientSubmitted": {
			"type": "boolean"
		},
		"submittedBy": {
			"$ref": "/schemas/provider-identity"
		},
		"pointValues": { "type": "object", "$comment": "TODO" },
		"totalScore": {
			"type": "number"
		}
	},
	"additionalProperties": false,
	"required": ["_type", "recordedDateTime", "scheduledAssessmentId", "assessmentId", "completed", "pointValues"]
}
