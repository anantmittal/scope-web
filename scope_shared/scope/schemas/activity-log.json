{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"$id": "https://uwscope.org/schemas/activity-log",
	"title": "IActivityLog",
	"description": "IActivityLog Type",
	"type": "object",
	"properties": {
		"_log_id": {
			"$ref": "/schemas/log#/properties/logId"
		},
		"_rev": {
			"type": "number"
		},
		"_type": {
			"const": "activityLog"
		},
		"recordedDate": {
			"$ref": "/schemas/log#/properties/recordedDate"
		},
		"comment": {
			"$ref": "/schemas/log#/properties/comment"
		},
		"scheduleId": {
			"type": "string"
		},
		"activityName": {
			"type": "string"
		},
		"completed": {
			"type": "boolean"
		},
		"success": {
			"$ref": "/schemas/enums#/properties/activitySuccessType"
		},
		"alternative": {
			"type": "string"
		},
		"pleasure": {
			"type": "number"
		},
		"accomplishment": {
			"type": "number"
		}
	},
	"additionalProperties": false,
	"required": ["_log_id", "_type", "_rev", "recordedDate", "scheduleId", "activityName"]
}
