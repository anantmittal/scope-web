{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"$id": "https://uwscope.org/schemas/activity-log",
	"title": "IActivityLog",
	"description": "IActivityLog Type",
	"type": "object",
	"properties": {
		"_id": {
			"type": "string"
		},
		"_type": {
			"const": "activityLog"
		},
		"_set_id": {
			"type": "string"
		},
		"_rev": {
			"type": "number"
		},
		"activityLogId": {
			"$ref": "/schemas/log#/properties/logId"
		},
		"recordedDateTime": {
			"$ref": "/schemas/log#/properties/recordedDateTime"
		},
		"comment": {
			"$ref": "/schemas/log#/properties/comment"
		},
		"scheduledActivityId": {
			"type": "string"
		},
		"activityName": {
			"type": "string"
		},
		"completed": {
			"type": "boolean"
		},
		"success": {
			"$ref": "/schemas/enums#/properties/activitySuccessType"
		},
		"alternative": {
			"type": "string"
		},
		"pleasure": {
			"type": "number"
		},
		"accomplishment": {
			"type": "number"
		}
	},
	"additionalProperties": false,
	"required": ["_type", "recordedDateTime", "scheduledActivityId", "activityName"]
}
