{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"$id": "https://uwscope.org/schemas/values-inventory",
	"title": "IValuesInventory",
	"description": "IValuesInventory Type",
	"type": "object",
	"properties": {
		"_id": {
			"type": "string"
		},
		"_type": {
			"const": "valuesInventory"
		},
		"_rev": {
			"type": "number"
		},
		"assigned": {
			"type": "boolean"
		},
		"assignedDate": {
			"$ref": "/schemas/datetime#/properties/date"
		},
		"lastUpdatedDate": {
			"$ref": "/schemas/datetime#/properties/date"
		},
		"values": {
			"type": "array",
			"items": {
				"$ref": "/schemas/life-area-value"
			}
		}
	},
	"additionalProperties": false,
	"required": ["_type", "assigned", "assignedDate"]
}
