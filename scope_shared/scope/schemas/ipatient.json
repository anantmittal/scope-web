{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"$id": "https://example.com/schemas/ipatient",
	"title": "IPatient",
	"description": "Holds all the patient data",
	"type": "object",
	"properties": {
		"identity": {
			"type": "object",
			"description": "IIdentity schema",
			"$ref": "/schemas/iidentity"
		},
		"profile": {
			"type": "object",
			"description": "IPatientProfile schema",
			"$ref": "/schemas/ipatient-profile"
		},
		"clinicalHistory": {
			"type": "object",
			"description": "IClinicalHistory schema",
			"$ref": "/schemas/iclinical-history"
		},
		"valuesInventory": {
			"type": "object",
			"description": "IValuesInventory schema",
			"$ref": "/schemas/ivalues-inventory"
		},
		"safetyPlan": {
			"type": "object",
			"description": "ISafetyPlan schema",
			"$ref": "/schemas/isafety-plan"
		},
		"sessions": { "type": "array" },
		"caseReviews": { "type": "array" },
		"assessments": { "type": "array" },
		"scheduledAssessments": { "type": "array" },
		"assessmentLogs": { "type": "array" },
		"activities": { "type": "array" },
		"scheduledActivities": { "type": "array" },
		"activityLogs": { "type": "array" },
		"moodLogs": { "type": "array" }
	},
	"additionalProperties": false,
	"required": ["identity", "profile", "clinicalHistory", "valuesInventory", "safetyPlan"]
}
