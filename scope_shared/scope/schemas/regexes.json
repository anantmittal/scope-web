{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"$id": "https://uwscope.org/schemas/regexes",
	"title": "Regexes",
	"type": "object",
	"properties": {
		"date": {
			"type": "string",
			"pattern": "^([0-9]{4})-(0[1-9]|1[0-2])-(0[1-9]|[12][0-9]|3[01])(T00:00:00)$",
			"$comment": "Allows 'YYYY-MM-DDT00:00:00'"
		}
	}
}
